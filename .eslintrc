{
  "extends": [ "airbnb", "plugin:react-native/all" ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "react-native",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "no-console": 0,
    "indent": ["error", 2],
    "linebreak-style": ["error","unix"],
    "quotes": ["error", "double"],
    "semi": ["error","always"],
    "global-require": 0,
    "import/prefer-default-exports": 0
  },
  "globals": {
    "__DEV__": true
  }
}